{
    "name": "abshammeri/terminal",
    "description": "Terminal is an Elegent wrapper around Symfony's Process component.",
    "keywords": ["laravel", "symfony", "process", "terminal", "bash", "exec"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/TitasGailius/terminal/issues",
        "source": "https://github.com/TitasGailius/terminal"
    },
    "authors": [
        {
            "name": "Titas Gailius",
            "email": "titas.gailius@gmail.com"
        }
    ],
    "require": {
        "symfony/process":  "^4.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.3.1",
        "symfony/var-dumper":  "^4.3.4"
    },
    "autoload": {
        "psr-4": {
            "TitasGailius\\Terminal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TitasGailius\\Terminal\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
